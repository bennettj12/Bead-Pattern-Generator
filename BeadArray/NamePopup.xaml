<Window x:Class="BeadArray.NamePopup"
		Loaded="Window_Loaded"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeadArray"
        mc:Ignorable="d"
        Title="Enter Palette Name" Height="200" Width="300">

	<StackPanel>
		<TextBlock Margin="5" Text="Enter Palette Name" />
		<TextBox Margin="5" x:Name="ResponseTextBox" KeyDown="ResponseTextBox_KeyDown"/>
		<Button Margin="5" Content="OK" Click="Palette_Name_Confirm"/>
	</StackPanel>
</Window>
